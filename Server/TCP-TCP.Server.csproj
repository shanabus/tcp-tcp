    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="7.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Client\TCP-TCP.Client.csproj" />
    <ProjectReference Include="..\Shared\TCP-TCP.Shared.csproj" />
  </ItemGroup>

</Project>
